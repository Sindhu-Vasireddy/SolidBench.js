{
  "@context": "https://linkedsoftwaredependencies.org/bundles/npm/sparql-query-parameter-instantiator/^1.0.0/components/context.jsonld",
  "@id": "urn:sparql-query-parameter-instantiator:default",
  "@type": "QueryInstantiator",
  "QueryInstantiator:_count": 5,
  "QueryInstantiator:_providers": [
    {
      "@type": "QueryTemplateProvider",
      "QueryTemplateProvider:_templateFilePath": "templates/queries/interactive-short-4.sparql",
      "QueryTemplateProvider:_destinationFilePath": "out-queries/interactive-short-4.sparql",
      "QueryTemplateProvider:_variables": [
        {
          "@type": "VariableTemplateNamedNode",
          "VariableTemplateNamedNode:_name": "message",
          "VariableTemplateNamedNode:_substitutionProvider": {
            "@type": "SubstitutionProviderCsv",
            "SubstitutionProviderCsv:_csvFilePath": "out-enhanced/parameters-posts.csv",
            "SubstitutionProviderCsv:_columnName": "post"
          },
          "VariableTemplateNamedNode:_valueTransformers": [
            {
              "@id": "urn:sparql-query-parameter-instantiator:valueTransformer1",
              "@type": "ValueTransformerReplaceIri",
              "ValueTransformerReplaceIri:_searchRegex": "^http://www.ldbc.eu",
              "ValueTransformerReplaceIri:_replacementString": "http://localhost:3000/www.ldbc.eu"
            },
            {
              "@id": "urn:sparql-query-parameter-instantiator:valueTransformer2",
              "@type": "ValueTransformerReplaceIri",
              "ValueTransformerReplaceIri:_searchRegex": "^http://dbpedia.org",
              "ValueTransformerReplaceIri:_replacementString": "http://localhost:3000/dbpedia.org"
            },
            {
              "@id": "urn:sparql-query-parameter-instantiator:valueTransformer3",
              "@type": "ValueTransformerReplaceIri",
              "ValueTransformerReplaceIri:_searchRegex": "^http://www.w3.org/2002/07/owl",
              "ValueTransformerReplaceIri:_replacementString": "http://localhost:3000/www.w3.org/2002/07/owl"
            }
          ]
        }
      ]
    },
    {
      "@type": "QueryTemplateProvider",
      "QueryTemplateProvider:_templateFilePath": "templates/queries/interactive-short-5.sparql",
      "QueryTemplateProvider:_destinationFilePath": "out-queries/interactive-short-5.sparql",
      "QueryTemplateProvider:_variables": [
        {
          "@type": "VariableTemplateNamedNode",
          "VariableTemplateNamedNode:_name": "message",
          "VariableTemplateNamedNode:_substitutionProvider": {
            "@type": "SubstitutionProviderCsv",
            "SubstitutionProviderCsv:_csvFilePath": "out-enhanced/parameters-posts.csv",
            "SubstitutionProviderCsv:_columnName": "post"
          },
          "VariableTemplateNamedNode:_valueTransformers": [
            {
              "@id": "urn:sparql-query-parameter-instantiator:valueTransformer1"
            },
            {
              "@id": "urn:sparql-query-parameter-instantiator:valueTransformer2"
            },
            {
              "@id": "urn:sparql-query-parameter-instantiator:valueTransformer3"
            }
          ]
        }
      ]
    }
  ]
}
