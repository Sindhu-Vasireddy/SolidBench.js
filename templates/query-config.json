{
  "@context": "https://linkedsoftwaredependencies.org/bundles/npm/sparql-query-parameter-instantiator/^2.0.0/components/context.jsonld",
  "@id": "urn:sparql-query-parameter-instantiator:default",
  "@type": "QueryInstantiator",
  "count": 5,
  "providers": [
    {
      "@type": "QueryTemplateProvider",
      "templateFilePath": "templates/queries/interactive-short-4.sparql",
      "destinationFilePath": "out-queries/interactive-short-4.sparql",
      "variables": [
        {
          "@type": "VariableTemplateNamedNode",
          "name": "message",
          "substitutionProvider": {
            "@type": "SubstitutionProviderCsv",
            "csvFilePath": "out-enhanced/parameters-posts.csv",
            "columnName": "post"
          },
          "valueTransformers": [
            {
              "@id": "urn:sparql-query-parameter-instantiator:valueTransformer1",
              "@type": "ValueTransformerReplaceIri",
              "searchRegex": "^http://www.ldbc.eu",
              "replacementString": "http://localhost:3000/www.ldbc.eu"
            },
            {
              "@id": "urn:sparql-query-parameter-instantiator:valueTransformer2",
              "@type": "ValueTransformerReplaceIri",
              "searchRegex": "^http://dbpedia.org",
              "replacementString": "http://localhost:3000/dbpedia.org"
            },
            {
              "@id": "urn:sparql-query-parameter-instantiator:valueTransformer3",
              "@type": "ValueTransformerReplaceIri",
              "searchRegex": "^http://www.w3.org/2002/07/owl",
              "replacementString": "http://localhost:3000/www.w3.org/2002/07/owl"
            }
          ]
        }
      ]
    },
    {
      "@type": "QueryTemplateProvider",
      "templateFilePath": "templates/queries/interactive-short-5.sparql",
      "destinationFilePath": "out-queries/interactive-short-5.sparql",
      "variables": [
        {
          "@type": "VariableTemplateNamedNode",
          "name": "message",
          "substitutionProvider": {
            "@type": "SubstitutionProviderCsv",
            "csvFilePath": "out-enhanced/parameters-posts.csv",
            "columnName": "post"
          },
          "valueTransformers": [
            {
              "@id": "urn:sparql-query-parameter-instantiator:valueTransformer1"
            },
            {
              "@id": "urn:sparql-query-parameter-instantiator:valueTransformer2"
            },
            {
              "@id": "urn:sparql-query-parameter-instantiator:valueTransformer3"
            }
          ]
        }
      ]
    }
  ]
}
